// https://github.com/2captcha/2captcha-php

require(__DIR__ . '/../src/autoloader.php');

$solver = new \TwoCaptcha\TwoCaptcha('YOUR_API_KEY');

try {

    $result = $solver->normal('path/to/captcha.jpg');

} catch (\Exception $e) {

    die($e->getMessage());

}

die('Captcha solved: ' . $result->code);
